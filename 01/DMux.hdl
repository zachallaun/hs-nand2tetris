/**
 * Demultiplexor.
 * {a,b} = {in,0} if sel==0
 *         {0,in} if sel==1
 */


CHIP DMux {
    IN in, sel;
    OUT a, b;

    PARTS:
    // a = notsel and in
    And(a=notsel, b=in, out=a);
    Not(in=sel, out=notsel);
    // b = sel and in
    And(a=sel, b=in, out=b);
}
